				<Value><![CDATA[
    SET NOCOUNT ON;

	DECLARE @current_change DateTime
	SET @current_change = GETDATE()

	UPDATE 
		[DataTypes] 
	SET 
		[datatype_id] = @datatype_id, 
		[name] = @name, 
		[description] = @description,
		[last_changed]         = @current_change
	WHERE 
	(
		([datatype_id] = @datatype_id)
		AND ([last_changed] = @last_changed)
	);

